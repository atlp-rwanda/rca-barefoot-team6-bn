{
  "name": "rca-barefoot-team6-bn",
  "version": "1.0.0",
  "description": "Project Development",
  "main": "app.js",
  "scripts": {
    "start": "node dist/app.js",
    "test": "mocha --require ts-node/register './__tests__/**/*.spec.ts'",
    "dev": "ts-node src/index.ts",
    "build": "rimraf ./build && tsc",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint --fix --ext .js",
    "sequelize": "sequelize-cli",
    "db:create": "sequelize-cli db:create",
    "db:migrate": "sequelize-cli db:migrate",
    "db:g:migration": "sequelize-cli migration:generate --name",
    "db:migrate:reset": "sequelize-cli db:migrate:undo:all && npm run db:migrate",
    "db:g:seed": "sequelize-cli seed:generate --name",
    "db:seeds": "sequelize-cli db:seed:all",
    "seeding": "npm run db:migrate:reset && npm run db:seeds"
  },
  "repository": {
    "type": "git",
    "url": "git@me:atlp-rwanda/rca-barefoot-team6-bn"
  },
  "keywords": [
    "React",
    "Hooks",
    "NodeJS",
    "PostgreSQL",
    "ExpressJS"
  ],
  "author": "Didier Munezero",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.29.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.21.0",
    "@types/chai": "^4.3.4",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.3",
    "@types/request": "^2.48.8",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "chai": "^4.3.7",
    "eslint": "^8.35.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^2.0.21",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.21",
    "request": "^2.88.2",
    "rimraf": "^4.4.0",
    "sequelize-cli": "^6.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
